    Public Function IniciarSession(ByVal usuario_ As String,
                              ByVal contrasena_ As String,
                              ByVal recordarSesion_ As String) As TagWatcher

        Dim tagwatcher_ As New TagWatcher

        _datosUsuario.Clear()

        _cantidadRegistros.Clear()

        'Se autentica por primera vez, el usuario y password.

        _userProfile = _wsSesion.GetProfileWorkSpace(_espacioTrabajoExtranet, usuario_, usuario_, contrasena_, 13, 1, 1, True)

        If _userProfile.Result.Status = Wma.Exceptions.TagWatcher.TypeStatus.Ok Then

            If _espacioTrabajoExtranet.SectorEntorno Is Nothing Or
               _espacioTrabajoExtranet.SectorEntorno.Count = 0 Then

                'El usuario es notificado que no tiene acceso porque seá necesario construir su perfil
                _userProfile.Result.SetError(Wma.Exceptions.TagWatcher.ErrorTypes.KBW_040_0002)

                Dim listaDestinatarios_ As New List(Of String)

                listaDestinatarios_.Add("pedro.bautista@kromaduanal.com")

                listaDestinatarios_.Add("alexis.herrera@kromaduanal.com")

                If EnviarCorreo(listaDestinatarios_,
                                "Inicio de sesión incompleto, (solicitud KrombaseWeb de " & usuario_ & ")",
                                "Hemos recibido una solicitud de acceso del usuario [" & usuario_ & "], con la contraseña [" & contrasena_ & "]" & Chr(13) &
                                " los datos del usuario son correctos, sin embargo el perfil no está completo, será necesario procesarlo y notificar a su cuenta de correo posteiormente.",
                                usuario_,
                                contrasena_,
                                Nothing,
                                _espacioTrabajoExtranet) Then


                End If

                Session("fallaLogin_") = _userProfile.Result.ErrorDescription

                tagwatcher_.SetError(Me, "¡Oops! Datos incorrectos")
            Else
                'Una vez que autentico correctamente y además tiene su carpeta del perfil correctamente cargada.
                'Si contiene la construcción dinámica, procedemos a crear el sitio

                Session("usuario") = usuario_

                Session("contrasena") = contrasena_

                If recordarSesion_ = "on" Then

                    _accessControl.GuardarConexion(New Dictionary(Of String, String) From {{"usuario", usuario_},
                                                                                             {"contrasena", contrasena_}})

                End If

                With _datosUsuario

                    .Add("MobilUserID", usuario_)

                    .Add("WebServiceUserID", usuario_)

                    .Add("WebServicePasswordID", contrasena_)

                    .Add("Nombre", _userProfile.Name & " " & _userProfile.PLastName)

                    .Add("Compañia", _userProfile.CompanyName)

                    .Add("Telefono", _userProfile.Phone)

                    .Add("Cumpleaños", _userProfile.BirthDate)

                    .Add("Correo", _userProfile.EMail)

                    .Add("Imagen", _userProfile.URLUserPicture)

                End With

                _cantidadRegistros.Add("LimiteRegistrosColeccionMuestral", 100)

                _cantidadRegistros.Add("LimiteResultados", 10000)

                _cantidadRegistros.Add("LimiteMuestralMinimo", 20)

                _credenciales = _espacioTrabajoExtranet.MisCredenciales


                'Aqui ya está construido el espacio de trabajo
                _controladorWeb = New ControladorWeb(_espacioTrabajoExtranet)


                tagwatcher_.SetOK()

                'Esta linea es requerida para Kromabaseweb, las consultas trabajan con las reglas del usuario MOP 04/05/2022
                If Not _controladorWeb.ClausulasSQLReglasAcceso Is Nothing Then

                    Session("DatosUsuario") = _datosUsuario

                    Session("CantidadRegistros") = _cantidadRegistros

                    'Aquí se entregan los objetos para disponer de ellos en el resto de la sesión
                    Session("EspacioTrabajoExtranet") = _espacioTrabajoExtranet '_wsSesion.EspacioTrabajo '_espacioTrabajoExtranet

                    Session("ControladorWeb") = _controladorWeb

                    If Not Session("redireccionamiento_url") Is Nothing Then

                        Response.Redirect(Session("redireccionamiento_url"))

                    End If

                    tagwatcher_.SetOKInfo(Me, _paginaInicial)

                Else

                    Session("fallaLogin_") = _userProfile.Result.ErrorDescription '"No se ha completado el registro del usuario. ( privilegios )"

                    tagwatcher_.SetError(Me, "¡Oops! Datos incorrectos")

                End If

            End If

        Else

            Session("fallaLogin_") = _userProfile.Result.ErrorDescription

            tagwatcher_.SetError(Me, "¡Oops! Datos incorrectos")

        End If

        Return tagwatcher_

    End Function

    Private Function IntentarConexionDesdeFormulario() As TagWatcher

        Dim tagwatcher_ As TagWatcher

        _usuario = Request.Form("user")

        _contrasena = Request.Form("password")

        If _usuario IsNot Nothing Then : _usuario = _usuario.Trim : End If

        If _contrasena IsNot Nothing Then : _contrasena = _contrasena.Trim : End If

        _recordarSesion = Request.Form("recordarSesion")

        If Not _usuario = Nothing And
            Not _contrasena = Nothing Then

            tagwatcher_ = IniciarSession(_usuario, _contrasena, _recordarSesion)

        Else

            tagwatcher_ = New TagWatcher(0, Me, "Faltan datos :(")

            Session("fallaLogin_") = "Por favor complete los datos para iniciar sesión"

        End If

        Return tagwatcher_

    End Function

