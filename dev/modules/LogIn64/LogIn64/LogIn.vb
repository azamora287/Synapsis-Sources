    <WebMethod>
    Public Shared Function CerrarSesion()

        Dim accessControl_ = New ControladorAccesoKBW()

        accessControl_.Desconectar()

        Return True

    End Function

    Public Sub EliminarSesionCompleta()

        Session("statusSession") = "void"

        Session("ControladorWeb") = Nothing

        Session("usuario") = Nothing

        Session("CantidadRegistros") = Nothing

        Session("password") = Nothing

        Session("DatosUsuario") = Nothing

        Session("fallaLogin_") = Nothing

        'Fetch the Cookie using its Key.
        Dim cookieSesion_ As HttpCookie = Request.Cookies("cookieSesion")

        If Not cookieSesion_ Is Nothing Then

            'Set the Expiry date to past date.
            cookieSesion_.Expires = DateTime.Now.AddDays(-1)

            'Update the Cookie in Browser.
            Response.Cookies.Add(cookieSesion_)

        End If

    End Sub

    Public Sub Redirect(Optional ByVal inicio_ As Boolean = False)

        Dim paginaInicial_ As String

        If inicio_ Then

            paginaInicial_ = _paginaLogIn

            Response.Redirect(paginaInicial_)

        Else

            paginaInicial_ = _paginaInicial

            Dim preferenciasUsuario_ As HttpCookie = Preferencias(MiSesion, PaginaReciente, crearSiNoExiste_:=False)

            If preferenciasUsuario_ IsNot Nothing Then

                If preferenciasUsuario_.Values(PaginaReciente.ToString) IsNot Nothing Then

                    Response.Redirect(preferenciasUsuario_.Values(PaginaReciente.ToString))

                Else

                    Response.Redirect(paginaInicial_)

                End If

            Else

                Response.Redirect(paginaInicial_)

            End If

        End If

    End Sub

    Protected Sub Page_Load(ByVal sender As Object, ByVal e As System.EventArgs) Handles Me.Load

        Dim status_ As String = Request.QueryString("sta")

        If status_ = "2" Then : Preferencias(MiSesion, eliminar_:=True) : End If

        Dim tagwatcher_ As TagWatcher

        If Not Me.IsPostBack Then

            If status_ Is Nothing Then

                If _accessControl.SesionAbierta() Then

                    Redirect()

                ElseIf _accessControl.IntentarConexion() Then

                    Redirect()

                End If

            End If

        Else

            If status_ = "0" Then

                tagwatcher_ = IntentarConexionDesdeFormulario()

                If tagwatcher_.Status = TagWatcher.TypeStatus.OkInfo Then

                    Redirect()

                Else

                    Redirect(inicio_:=True)

                End If

            Else
                '
                If _accessControl.SesionAbierta() Then

                    Redirect()

                ElseIf _accessControl.IntentarConexion() Then

                    Redirect()

                Else

                    tagwatcher_ = IntentarConexionDesdeFormulario()

                    If tagwatcher_.Status = TagWatcher.TypeStatus.OkInfo Then

                        Redirect()

                    Else

                        Redirect(inicio_:=True)

                    End If

                End If

            End If

        End If

    End Sub

